function nor_to_lowerCase($string) 
{
  $mapping = 
  [
      'A' => 'a', 'B' => 'b', 'C' => 'c', 'D' => 'd', 'E' => 'e',
      'F' => 'f', 'G' => 'g', 'H' => 'h', 'I' => 'i', 'J' => 'j',
      'K' => 'k', 'L' => 'l', 'M' => 'm', 'N' => 'n', 'O' => 'o',
      'P' => 'p', 'Q' => 'q', 'R' => 'r', 'S' => 's', 'T' => 't',
      'U' => 'u', 'V' => 'v', 'W' => 'w', 'X' => 'x', 'Y' => 'y',
      'Z' => 'z', '' => '', '' => '', '' => '', '' => '',
      '' => '', '' => '', '' => '', '' => '', '' => '',
      '' => '', '' => '', '' => '', '' => '', '' => '',
      '' => '', '' => ''
  ];

  $loweredString = '';
  $length = strlen($string);
  for ($i = 0; $i < $length; $i++) 
  {
      $char = $string[$i];
      if (isset($mapping[$char])) 
      {
          $char = $mapping[$char];
      }
      $loweredString .= $char;
  }

  return $loweredString;
}

