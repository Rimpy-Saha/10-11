<script>
function showMainCategory(categoryClass, buttonEl) {
  // 1) Hide all sections (keep your existing behavior)
  document.querySelectorAll('.products-suggestions').forEach(s => s.style.display = 'none');

  // 2) Scope to THIS selection group so multiple groups won't conflict
  const container = buttonEl.closest('.selection-div') || document;

  // 3) Reset all buttons in this selection group + reset svg styling
  container.querySelectorAll('button').forEach(b => {
    b.classList.remove('active');
    const svg = b.querySelector('svg');
    if (svg) {
      svg.querySelectorAll('path').forEach(p => {
        p.style.fill = '#3f444a';   // reset to default
        p.style.stroke = 'none';
      });
    }
  });
  // 4) Show the chosen section
  const target = document.querySelector('.' + categoryClass);
  if (target) {
    target.style.display = 'block';
    const blocks = target.querySelectorAll('.all-products');
    const anyVisible = Array.from(blocks).some(b => getComputedStyle(b).display !== 'none');
    if (!anyVisible && blocks.length) blocks[0].style.display = 'block';
    const firstBtn = target.querySelector('.subcat-btn');
    if (firstBtn) styleSubcatButtons(target, firstBtn);
  }

  // 5) Activate the clicked button and style ONLY its SVG
  buttonEl.classList.add('active');
  const svg = buttonEl.querySelector('svg');
  if (svg) {
    svg.querySelectorAll('path').forEach(p => {
      p.style.fill = '#004897';       // highlight color
      p.style.strokeWidth = '2px';
    });
  }
}




  function onSubcatClick(e) {
    const btn = e.currentTarget;
    const section = btn.closest('.products-suggestions');
    if (!section) return;

    section.querySelectorAll('.all-products').forEach(el => el.style.display = 'none');

    const targetSel = btn.getAttribute('data-target');
    const target = section.querySelector(targetSel);
    if (target) target.style.display = 'block';

    styleSubcatButtons(section, btn);
  }

  function styleSubcatButtons(section, activeBtn) {
    section.querySelectorAll('.subcat-btn').forEach(b => {
      b.style.background = 'white';
      b.style.borderColor = '#004897';
      b.style.color = 'black';
    });
    activeBtn.style.background = '#004897';
    activeBtn.style.borderColor = '#004897';
    activeBtn.style.color = 'white';

  }

  document.addEventListener('DOMContentLoaded', function () {
    document.querySelectorAll('.products-suggestions .subcat-btn').forEach(btn => {
      btn.addEventListener('click', onSubcatClick);
    });

    const defaultTopBtn = document.querySelector('.category-btn-rna');
    showMainCategory('collection-preservation-cat', defaultTopBtn);
  });
</script>





